{
  "name": "quandary-lang",
  "displayName": "Quandary language",
  "description": "Language support for Quandary",
  "version": "1.0.1",
  "publisher": "cameroncromer",
  "repository": {
    "type": "git",
    "url": "https://github.com/cameroncromer/vscode-quandary.git"
  },
  "license": "SEE LICENSE IN LICENSE",
  "icon": "./images/icon.png",
  "galleryBanner": {
    "color": "#A7B1B7",
    "theme": "light"
  },
  "engines": {
    "vscode": "^1.75.1"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "quandary",
        "aliases": [
          "Quandary",
          "quandary"
        ],
        "extensions": [
          ".q",
          ".qet"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./images/icon.png",
          "dark": "./images/icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "quandary",
        "scopeName": "source.q",
        "path": "./syntaxes/quandary.tmLanguage.json"
      }
    ]
  }
}